<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA"
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<fieldProperty
			name="esercizio"
			property="esercizio"
			inputSize="5"
			inputType="ROTEXT"
			label="Esercizio" />
	<fieldProperty
			name="ti_gestione"
			property="ti_gestione"
			inputType="RADIOGROUP"
			keysProperty="ti_gestioneKeys"
			nullable="false"
			enabledOnSearch="true"
			command="doOnTipoChange"
			layout="HORIZONTAL"
			label="Tipo Stampa" />
	<fieldProperty
			name="ti_stampa"
			property="ti_stampa"
			inputType="RADIOGROUP"
			keysProperty="ti_stampaKeys"
			nullable="false"
			enabledOnSearch="true"
			command="doOnTipoBilancioChange"
			layout="HORIZONTAL"
			label="Tipo Bilancio" />
	<fieldProperty
			name="ti_aggregazione"
			property="ti_aggregazione"
			inputType="RADIOGROUP"
			keysProperty="ti_aggregazioneKeys"
			nullable="false"
			enabledOnSearch="true"
			command="doOnTipoAggregazioneChange"
			layout="HORIZONTAL"
			label="Tipo Aggregazione" />
	<fieldProperty
			name="ti_origine"
			property="ti_origine"
			inputType="RADIOGROUP"
			keysProperty="ti_origineKeys"
			nullable="false"
			enabledOnSearch="true"
			layout="HORIZONTAL"
			label="Fonte Dati" />
	<fieldProperty
			name="livello_stampa"
			property="livello_stampa"
			inputSize="5"
			inputType="ROTEXT"
			label="Livello Stampa" />

	<fieldProperty
			name="find_voce_da"
			property="voce_da"
			inputType="SEARCHTOOL"
			formName="searchtool"
			label="Voce Da"
			enabledOnSearch="true"
			completeOnSave="false"
			nullable="false"
	/>
	<fieldProperty
			name="cd_voce_da"
			property="voce_da.cd_voce_da"
			label="Voce del piano"
			readonlyProperty="ROVoce_da"
			inputType="TEXT"
			maxLength="20"
			inputSize="20"
			enabledOnSearch="true"
			nullable="false"
	/>
	<fieldProperty
			name="cd_voce_daRO"
			property="voce_da.cd_voce_da"
			label="Voce del piano"
			inputType="ROTEXT"
			maxLength="20"
			inputSize="20"
	/>
	<fieldProperty
			name="ds_voce_da"
			property="voce_da.ds_voce_da"
			label=""
			readonlyProperty="ROVoce_da"
			inputType="ROTEXT"
			inputSize="60"
	/>


	<fieldProperty
			name="find_voce_a"
			property="voce_a"
			inputType="SEARCHTOOL"
			formName="searchtool"
			label="Voce a"
			enabledOnSearch="true"
			completeOnSave="false"
			nullable="false"
	/>
	<fieldProperty
			name="cd_voce_a"
			property="voce_a.cd_voce_a"
			label="Voce A"
			readonlyProperty="ROVoce_a"
			inputType="TEXT"
			maxLength="20"
			inputSize="20"
			enabledOnSearch="true"
			nullable="false"
	/>
	<fieldProperty
			name="cd_voce_aRO"
			property="voce_a.cd_voce_a"
			label="Voce a"
			inputType="ROTEXT"
			maxLength="20"
			inputSize="20"
	/>
	<fieldProperty
			name="ds_voce_a"
			property="voce_a.ds_voce_a"
			label=""
			readonlyProperty="ROVoce_a"
			inputType="ROTEXT"
			inputSize="60"
	/>

	<formFieldProperty name="esercizio"/>
	<formFieldProperty name="ti_gestione" />
	<formFieldProperty name="ti_stampa" />
	<formFieldProperty name="ti_aggregazione" />
	<formFieldProperty name="ti_origine" />
	<formFieldProperty name="livello_stampa" />
	<formFieldProperty name="find_voce_da" />
	<formFieldProperty name="find_voce_a" />
	<formFieldProperty name="voce_da" />
	<formFieldProperty name="voce_a" />

	<printForm
			name="/preventivo/preventivo/mastro.jasper">
		<printFieldProperty
				name="esercizio"
				parameterPosition="0"
				parameterType="STRING"
				paramNameJR="P_ESERCIZIO"
				paramTypeJR="java.lang.Integer" />
		<printFieldProperty
				name="ti_gestione"
				property="ti_gestione"
				parameterPosition="1"
				parameterType="STRING"
				paramNameJR="P_TIPO"
				paramTypeJR="java.lang.String" />
		<printFieldProperty
				name="ti_stampa"
				property="ti_stampa"
				parameterPosition="2"
				parameterType="STRING"
				paramNameJR="P_TIPO_RENDICONTO"
				paramTypeJR="java.lang.String" />
		<printFieldProperty
				name="ti_aggregazione"
				property="ti_aggregazione"
				parameterPosition="3"
				parameterType="STRING"
				paramNameJR="P_TIPO_AGGREGAZIONE"
				paramTypeJR="java.lang.String" />
		<printFieldProperty
				name="ti_origine"
				property="ti_origine"
				parameterPosition="4"
				parameterType="STRING"
				paramNameJR="P_ORIGINE"
				paramTypeJR="java.lang.String" />
		<printFieldProperty
				name="livello_stampa"
				property="livello_stampa"
				parameterPosition="5"
				parameterType="INTEGER"
				paramNameJR="P_NUM_LIV"
				paramTypeJR="java.lang.Integer" />
		<printFieldProperty
				name="voce_da"
				property="voce_da.cd_elemento_voce"
				parameterPosition="6"
				parameterType="STRING"
				paramNameJR="P_VOCE_DA"
				paramTypeJR="java.lang.String" />
		<printFieldProperty
				name="voce_a"
				property="voce_a.cd_elemento_voce"
				parameterPosition="7"
				parameterType="STRING"
				paramNameJR="P_VOCE_A"
				paramTypeJR="java.lang.String" />
	</printForm>

</bulkInfo>