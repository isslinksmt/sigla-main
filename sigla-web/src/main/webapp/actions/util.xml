<action-mappings>
	<action
			path="/ModalAction" actionClass="it.cnr.contab.util00.action.ModalAction">
	</action>
	<businessProcess name="ModalBP" className="it.cnr.contab.util00.bp.ModalBP">
		<forward name="default" path="/util00/modal.jsp"/>
		<init-param name="componentSessionName" value="JADAEJB_CRUDComponentSession"/>
		<init-param name="defaultAction" value="ModalAction"/>
	</businessProcess>

	<action
			path="/ModalitaPagamentoModalAction" actionClass="it.cnr.contab.util00.action.ModalitaPagamentoModalAction">
	</action>
	<businessProcess name="ModalitaPagamentoModalBP" className="it.cnr.contab.util00.bp.ModalitaPagamentoModalBP">
		<forward name="default" path="/util00/modalita_pagamento_modal.jsp"/>
		<init-param name="componentSessionName" value="JADAEJB_CRUDComponentSession"/>
		<init-param name="defaultAction" value="ModalitaPagamentoModalAction"/>
	</businessProcess>

</action-mappings>